{% extends 'base.html.twig' %}
{% import "_macros/breadcumb.html.twig" as _breadcumb %}

{% block title %}Les Entreprises{% endblock %}
{% block titre_page %}Ajout d'une entreprise{% endblock %}
{% block action_page %}
    <a href="{{ path('app_entreprise_index') }}" class="btn btn-sm btn-primary ms-3 px-4 py-3" >Retour</a>
{% endblock %}
{% block Breadcrumb %}
    {{ _breadcumb.etape("Entreprise") }}
    {{ _breadcumb.etape("Ajout d'une nouvelle entreprise") }}
{% endblock %}
{% block stylesheets %}
    {{ parent() }}
{% endblock %}

{% block body %}
    {{ include('entreprise/_form.html.twig') }}
{% endblock %}

{% block javascripts %}
    {{ parent() }}
    <script src="{{ asset('js/adresse_autocomplete.js') }}"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js"></script>
{% endblock %}

