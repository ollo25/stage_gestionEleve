{% extends 'base.html.twig' %}
{% import "_macros/breadcumb.html.twig" as _breadcumb %}

{% block title %}Les stages{% endblock %}
{% block titre_page %}Ajout d'un nouveau stage{% endblock %}
{% block action_page %}
    <a href="{{ path('app_stage_index') }}" class="btn btn-sm btn-primary ms-3 px-4 py-3" >Retour</a>
{% endblock %}
{% block Breadcrumb %}
    {{ _breadcumb.etape("Stage") }}
    {{ _breadcumb.etape("Ajout d'un nouveau stage") }}
{% endblock %}
{% block stylesheets %}
    {{ parent() }}
{% endblock %}

{% block body %}
    {{ include('stage/_form.html.twig') }}
{% endblock %}

{% block javascripts %}
    {{ parent() }}
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js"></script>
{% endblock %}
