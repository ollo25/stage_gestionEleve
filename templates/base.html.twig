
{% block stylesheets %}
    <link rel="stylesheet" href="{{ asset('styles/base.css') }}">
    <link rel="stylesheet" href="{{ asset('styles/tableau.css') }}">
    <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/1.13.6/css/jquery.dataTables.min.css"/>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css">
    <link rel="stylesheet" href="{{ asset('styles/style.css') }}">
{% endblock %}
{% block javascripts %}
    <script src="{{ asset('js/base.js') }}"></script>

{% endblock %}
<nav class="animated-nav">
    <div class="tab-content">
        <ul class="nav-list">
            <li class="nav-item {% if app.request.attributes.get('_route') == 'app_etudiant_index' %}active{% endif %}" data-target="home">
                <a href="{{ path('app_etudiant_index') }}">Élèves</a>
            </li>
            <li class="nav-item {% if app.request.attributes.get('_route') == 'app_potentiel_eleve_index' %}active{% endif %}" data-target="products">
                <a href="{{ path('app_potentiel_eleve_index') }}">Potentiels Élèves</a>
            </li>
            <li class="nav-item {% if app.request.attributes.get('_route') == 'app_entreprise_index' %}active{% endif %}" data-target="services">
                <a href="{{ path('app_entreprise_index') }}">Entreprises</a>
            </li>
            <div class="nav-indicator"></div>
        </ul>
    </div>
</nav>
