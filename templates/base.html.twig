<script>
    toastr.options = {
        "closeButton": false,
        "debug": false,
        "newestOnTop": true,
        "progressBar": true,
        "positionClass": "toastr-top-right",
        "preventDuplicates": false,
        "onclick": null,
        "showDuration": "300",
        "hideDuration": "1000",
        "timeOut": "5000",
        "extendedTimeOut": "1000",
        "showEasing": "swing",
        "hideEasing": "linear",
        "showMethod": "fadeIn",
        "hideMethod": "fadeOut"
    };
    {% for type, messages in app.session.flashbag.all() %}
        {% for message in messages %}
            {% if type == 'danger' %}
                toastr.error( "{{ message|raw }}","Erreur");
            {% elseif type == 'warning' %}
                toastr.warning("{{ message|raw }}","Attention")
            {% elseif type == 'info' %}
                toastr.info("{{ message|raw }}","Pour information")
            {% else %}
                toastr.success("{{ message|raw }}","Super !")
            {% endif %}
            {# Or even with css class rendering:
                    <div class="flash-{{type}}">{{message}}</div>
            #}
        {% endfor %}
    {% endfor %}
</script>
